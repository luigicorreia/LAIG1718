        <TEXTURE id="wooden" />

        <SCALE sx="10" sy="1" sz="8" />
        <TRANSLATION x="0" y="0" z="4" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <LEAF  type="rectangle" args="0 4 5 0" />
        </DESCENDANTS>

    </NODE>

    <NODE id="carpet" selectable="true">
    	 <MATERIAL id="null" />
        <TEXTURE id="red_carpet" />


        <TRANSLATION x="0" y="0.01" z="31" />
        <ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <LEAF  type="rectangle" args="0 10 50 0" />
        </DESCENDANTS>

    </NODE>

    <NODE id="arrow">
