    	<TRANSLATION x="1" y="0" z="23"/>

    	<DESCENDANTS>
    		<LEAF type="triangle" args="0 0.05 -0.5 0 0.05 6.5 5 0.05 3" />
    	</DESCENDANTS>
    </NODE>


