<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.5" far="500"/>
    <translation x="-1" y="1" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.25" sy="0.25" sz="0.25" />
    <reference length="5" />
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />
    <background r="0" g="0" b="0" a="1" />
</ILLUMINATION>


<!-- Lights -->
<LIGHTS>
	<LIGHT id="main_light1">
        <enable value ="1" />
        <position  x="5" y="20" z="15" w="1" />
        <ambient r="0.6" g="0.6" b="0.6" a="1.0" />
        <diffuse r="0.5" g="0.5" b="0.5" a="0.5" />
        <specular r="0.2" g="0.2" b="0.2" a="1.0" />
    </LIGHT>

	<LIGHT id="main_light2">
        <enable value ="1" />
        <position x="45" y="20" z="20" w="1" />
        <ambient r="0.6" g="0.6" b="0.6" a="1.0" />
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
        <specular r="0.3" g="0.3" b="0.3" a="0.0" />
    </LIGHT>

    <LIGHT id="neil_frame">
        <enable value ="1" />
        <position x="1" y="9" z="13.5" w="1" />
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
        <specular r="0.5" g="0.5" b="0.5" a="1.0" />
