<MATERIALS>
	<!-- Pyramid material -->
    <MATERIAL id="piramideMaterial">
        <shininess value="1" />
        <specular r="0" g="0" b="0" a="1" />
        <diffuse r="1.0" g="0.68" b="0.4" a="1" />
        <ambient r="0.2" g="0.2" b="0.2" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Tree leaves material -->
	<MATERIAL id="leavesTreeMaterial">
        <shininess value="1" />
        <specular r="0.2" g="0.2" b="0.2" a="1" />
        <diffuse r="0.0" g="0.207" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

	<!-- Tree trunk material -->
	<MATERIAL id="trunkTreeMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.6" g="0.298" b="0.0" a="1" />
        <ambient r="0.1" g="0.1" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Lamp post material -->
    <MATERIAL id="lightPostMaterial">
        <shininess value="100" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.447" g="0.416" b="0.416" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- Light bulb material -->
    <MATERIAL id="lightMaterial">
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />
        <ambient r="0.0" g="0.0" b="0.0" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <!-- A default material -->
    <MATERIAL id="defaultMaterial">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />
        <ambient r="0.4" g="0.4" b="0.4" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="red">
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />
        <diffuse r="0.5" g="0" b="0.1" a="1" />
        <ambient r="0.5" g="0" b="0.1" a="1" />
        <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

</MATERIALS>

<ANIMATIONS>

  <ANIMATION id="fag1" speed="2" type="linear">
    <controlpoint xx="2" yy="3" zz="4" />
    <controlpoint xx="3" yy="5" zz="5" />
    <controlpoint xx="7" yy="2" zz="3" />
    <controlpoint xx="2" yy="2" zz="2" />
  </ANIMATION>

  <ANIMATION id="fag2" speed="2" type="circular"
    centerx="4" centery="0" centerz="4"
    radius="5" startang="21" rotang="360"/>

  <ANIMATION id="fag3" speed="2" type="bezier">
    <controlpoint xx="0" yy="3" zz="6" />
    <controlpoint xx="3" yy="3" zz="6" />
    <controlpoint xx="8" yy="0.5" zz="6" />
    <controlpoint xx="15" yy="0.5" zz="6" />
  </ANIMATION>

<ANIMATION id="linebetweenbezier" speed="2" type="linear">
    <controlpoint xx="15" yy="0.5" zz="6" />
    <controlpoint xx="10" yy="1.5" zz="6" />
</ANIMATION>

<ANIMATION id="bezier1" speed="2" type="bezier">
    <controlpoint xx="15" yy="0.5" zz="6" />
    <controlpoint xx="8" yy="0.5" zz="6" />
    <controlpoint xx="3" yy="3" zz="6" />
    <controlpoint xx="-1" yy="3" zz="6" />
</ANIMATION>

  <ANIMATION id="fag4" type="combo">
    <SPANREF id="fag1" />
    <SPANREF id="fag2" />
    <SPANREF id="fag3" />
  </ANIMATION>

<ANIMATION id="linear1" speed="4" type="linear">
  <controlpoint xx="0" yy="0" zz="5" />
  <controlpoint xx="5" yy="0" zz="5" />
</ANIMATION>

<ANIMATION id="linear2" speed="4" type="linear">
  <controlpoint xx="5" yy="0" zz="5" />
  <controlpoint xx="5" yy="0" zz="10" />
</ANIMATION>

  <ANIMATION id="circ1" speed="3" type="circular"
    centerx="5" centery="0" centerz="5"
    radius=".2" startang="0" rotang="90"/>

    <ANIMATION id="circ2" speed="3" type="circular"
    centerx="6" centery="0" centerz="5"
    radius="3" startang="0" rotang="90"/>

<ANIMATION id="combo1" type="combo">
  <SPANREF id="fag3" />
  <SPANREF id="linebetweenbezier" />
  <SPANREF id="linear1" />
</ANIMATION>

<ANIMATION id="combo2" type="combo">
     
    <SPANREF id="linear1" />
    <SPANREF id="circ1" />
    <SPANREF id="linear2" />
   
    
    
</ANIMATION>




</ANIMATIONS>

<!-- The leaves -->
<LEAVES>
</LEAVES>


<!-- The nodes -->
<NODES>

    <ROOT id="root" />

    <!-- The root node -->
