        <MATERIAL id="defaultMaterial" />
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="satellite_model" />
            <NODEREF id="floor" />
            <NODEREF id="earth_model" />
            <NODEREF id="sun_model" />
            <NODEREF id="walls" />
            <NODEREF id="rover" />
            <NODEREF id="moon_expo" />
            <NODEREF id="youshallnotpass" />
            <NODEREF id="saturn_model" />
        </DESCENDANTS>

    </NODE>



    <NODE id="floor">
    	<MATERIAL id="null" />
    	<TEXTURE id="null" />

    	<DESCENDANTS>
    		<NODEREF id="pavement" />
    		<NODEREF id="carpet" />
    		<NODEREF id="arrow" />
       	</DESCENDANTS>
    </NODE>

    <NODE id="pavement">
